<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context="kobeissidev.calculategpa.HighSchoolGPA"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <AutoCompleteTextView
        android:id="@+id/gradeLabelHS"
        android:layout_width="128dp"
        android:layout_height="42dp"
        android:editable="false"
        android:text="   Grade    "
        android:textAlignment="center"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/typeOfClassLabel"
        app:layout_constraintLeft_toLeftOf="@+id/linearLayout7"
        app:layout_constraintRight_toRightOf="@+id/linearLayout7"
        app:layout_constraintTop_toTopOf="@+id/typeOfClassLabel"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintVertical_bias="0.0" />

    <AutoCompleteTextView
        android:id="@+id/typeOfClassLabel"
        android:layout_width="128dp"
        android:layout_height="42dp"
        android:layout_marginStart="61dp"
        android:editable="false"
        android:text="Type of Class"
        android:textAlignment="center"
        android:visibility="visible"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="61dp" />

    <Button
        android:id="@+id/calcButtonHS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calculate GPA!"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8"
        tools:layout_constraintLeft_creator="1" />

    <EditText
        android:id="@+id/textGPAHS"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="none"
        android:singleLine="true"
        android:editable="false"
        android:textAlignment="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:divider="@drawable/empty_tall_divider"
        android:orientation="vertical"
        android:showDividers="middle"
        android:visibility="visible"
        android:weightSum="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/typeOfClassLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/typeOfClassLabel"
        android:id="@+id/linearLayout8">

        <Spinner
            android:id="@+id/tocOne"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="197dp" />

        <Spinner
            android:id="@+id/tocTwo"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="315dp" />

        <Spinner
            android:id="@+id/tocThree"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="116dp" />

        <Spinner
            android:id="@+id/tocFour"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

        <Spinner
            android:id="@+id/tocFive"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

        <Spinner
            android:id="@+id/tocSix"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:divider="@drawable/empty_tall_divider"
        android:orientation="vertical"
        android:showDividers="middle"
        android:visibility="visible"
        android:weightSum="1"
        android:id="@+id/linearLayout7"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/gradeLabelHS"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/typeOfClassLabel">

        <Spinner
            android:id="@+id/gradeOne"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="197dp" />

        <Spinner
            android:id="@+id/gradeTwo"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="315dp" />

        <Spinner
            android:id="@+id/gradeThree"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="116dp" />

        <Spinner
            android:id="@+id/gradeFour"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

        <Spinner
            android:id="@+id/gradeFive"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

        <Spinner
            android:id="@+id/gradeSix"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            android:visibility="gone"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
