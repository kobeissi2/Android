<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kobeissidev.calculatecollegegpa.MainActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="79dp"
        android:layout_height="0dp"
        android:layout_marginEnd="5dp"
        android:divider="@drawable/empty_tall_divider"
        android:orientation="vertical"
        android:showDividers="middle"
        android:weightSum="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout"
        android:id="@+id/linearLayout2">

        <Spinner
            android:id="@+id/first"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="197dp" />

        <Spinner
            android:id="@+id/second"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="315dp" />

        <Spinner
            android:id="@+id/third"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="116dp" />

        <Spinner
            android:id="@+id/fourth"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

        <Spinner
            android:id="@+id/fifth"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="79dp"
        android:layout_height="304dp"
        android:layout_marginEnd="31dp"
        android:divider="@drawable/empty_tall_divider"
        android:orientation="vertical"
        android:showDividers="middle"
        android:weightSum="1"
        android:id="@+id/linearLayout"
        app:layout_constraintRight_toLeftOf="@+id/linearLayout2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toTopOf="parent">

        <Spinner
            android:id="@+id/creditFirst"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="197dp" />

        <Spinner
            android:id="@+id/creditSecond"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="315dp" />

        <Spinner
            android:id="@+id/creditThird"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="116dp" />

        <Spinner
            android:id="@+id/creditFourth"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

        <Spinner
            android:id="@+id/creditFifth"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="0.02"
            tools:layout_editor_absoluteX="344dp"
            tools:layout_editor_absoluteY="143dp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
